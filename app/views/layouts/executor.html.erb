<!DOCTYPE html>
<html>
  <head>
    <title>flowychat :: execution</title>
    <meta name="viewport" content="width=device-width, user-scalable=no">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'executor', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'executor', 'data-turbolinks-track': 'reload' %>
  </head>

  <body 
    data-flow-id="<%= @flow_id %>" 
    data-authorization="<%= @user&.authorization_token %>" 
    data-execution-id="<%= @execution_id %>">
    <%= yield %>
  </body>
</html>
